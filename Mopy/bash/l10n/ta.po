#: Mopy/bash/ScriptParser.py:102
#, python-format
msgid "Too few arguments to %(obj_type)s '%(obj_name)s': got %(got)s, expected %(expected)s."
msgstr "%(got)s ' %(expected)s' க்கு மிகக் குறைவான வாதங்கள்: கிடைத்தது %(obj_type)s, எதிர்பார்க்கப்படும் %(obj_name)s."

#: Mopy/bash/ScriptParser.py:108
#, python-format
msgid "Too many arguments to %(obj_type)s '%(obj_name)s': got %(got)s, expected %(expected)s."
msgstr "%(got)s ' %(expected)s' க்கு பல வாதங்கள்: கிடைத்தது %(obj_type)s, எதிர்பார்க்கப்படும் %(obj_name)s."

#: Mopy/bash/ScriptParser.py:114
#, python-format
msgid "Cannot set %(obj_type)s '%(obj_name)s': type is '%(conflicting_type)s'."
msgstr "%(conflicting_type)s '%(obj_name)s' ஐ அமைக்க முடியாது: வகை '%(obj_type)s'."

#: Mopy/bash/ScriptParser.py:120
#, python-format
msgid "Unexpected '%(unexpected_keyword)s'."
msgstr "எதிர்பாராத '%(unexpected_keyword)s'."

#: Mopy/bash/ScriptParser.py:124
#, python-format
msgid "Function '%(string_only_function)s' only operates on string types."
msgstr "செயல்பாடு '%(string_only_function)s' சரம் வகைகளில் மட்டுமே இயங்குகிறது."

#: Mopy/bash/ScriptParser.py:513 Mopy/bash/ScriptParser.py:743
msgid "Index out of bounds."
msgstr "எல்லைக்கு வெளியே குறியீட்டு."

#: Mopy/bash/ScriptParser.py:603
msgid "Mismatched parenthesis."
msgstr "பொருந்தாத அடைப்புக்குறி."

#: Mopy/bash/ScriptParser.py:609
msgid "Mismatched brackets."
msgstr "பொருந்தாத அடைப்புக்குறிகள்."

#: Mopy/bash/ScriptParser.py:666
msgid "Misplaced ',' or missing parenthesis."
msgstr "தவறான ',' அல்லது காணாமல் போன அடைப்புக்குறிப்பு."

#: Mopy/bash/ScriptParser.py:675
msgid "Misplaced ':' or missing bracket."
msgstr "தவறாக ':' அல்லது காணாமல் போன அடைப்புக்குறி."

#: Mopy/bash/ScriptParser.py:689
msgid "Dot operator: no function to call."
msgstr "டாட் ஆபரேட்டர்: அழைக்க எந்த செயல்பாடும் இல்லை."

#: Mopy/bash/ScriptParser.py:692
#, python-format
msgid "Dot operator: cannot access non-function '%(other_token)s'."
msgstr "டாட் ஆபரேட்டர்: செயல்படாத '%(other_token)s' ஐ அணுக முடியாது."

#: Mopy/bash/ScriptParser.py:696
#, python-format
msgid "Dot operator: cannot access function '%(non_dot_function)s'."
msgstr "டாட் ஆபரேட்டர்: செயல்பாட்டை அணுக முடியாது '%(non_dot_function)s'."

#: Mopy/bash/ScriptParser.py:723 Mopy/bash/ScriptParser.py:753
msgid "Unmatched parenthesis."
msgstr "ஒப்பிடமுடியாத அடைப்புக்குறிப்பு."

#: Mopy/bash/ScriptParser.py:734
msgid "Unmatched brackets."
msgstr "ஒப்பிடமுடியாத அடைப்புக்குறிகள்."

#: Mopy/bash/ScriptParser.py:746
#, python-format
msgid "Unrecognized token: '%(unknown_token_text)s', type: %(unknown_token_type)s"
msgstr "அங்கீகரிக்கப்படாத டோக்கன்: ' %(unknown_token_type)s', வகை: %(unknown_token_text)s"

#: Mopy/bash/ScriptParser.py:793
msgid "Too many values left at the end of evaluation."
msgstr "மதிப்பீட்டின் முடிவில் பல மதிப்புகள் உள்ளன."

#: Mopy/bash/ScriptParser.py:864
#, python-format
msgid "Invalid character: '%(invalid_char)s'"
msgstr "தவறான எழுத்து: '%(invalid_char)s'"

#: Mopy/bash/ScriptParser.py:873
msgid "Unterminated single quote."
msgstr "வழங்கப்படாத ஒற்றை மேற்கோள்."

#: Mopy/bash/ScriptParser.py:888
msgid "Unterminated double quote."
msgstr "வழங்கப்படாத இரட்டை மேற்கோள்."

#: Mopy/bash/ScriptParser.py:909
#, python-format
msgid "Unexpected quotation mark (%(wrong_mark)s) following name token."
msgstr "எதிர்பாராத மேற்கோள் குறி (%(wrong_mark)s) பின்வரும் பெயர் கிள்ளாக்கு."

#: Mopy/bash/ScriptParser.py:930
#, python-format
msgid "Unexpected quotation mark (%(wrong_mark)s) following number token."
msgstr "எதிர்பாராத மேற்கோள் குறி (%(wrong_mark)s) பின்வரும் எண் கிள்ளாக்கு."

#: Mopy/bash/ScriptParser.py:939
#, python-format
msgid "Unexpected symbol (%(wrong_symbol)s) following decimal token."
msgstr "தசம டோக்கனைத் தொடர்ந்து எதிர்பாராத அடையாளம் (%(wrong_symbol)s)."

#: Mopy/bash/ScriptParser.py:945
#, python-format
msgid "Unexpected quotation mark (%(wrong_mark)s) following parenthesis."
msgstr "அடைப்புக்குறிப்பைத் தொடர்ந்து எதிர்பாராத மேற்கோள் குறி (%(wrong_mark)s."

#: Mopy/bash/ScriptParser.py:1081 Mopy/bash/basher/belt.py:838
msgid "No reason given"
msgstr "எந்த காரணமும் கொடுக்கப்படவில்லை"

#: Mopy/bash/ScriptParser.py:1088
#, python-format
msgid "Cannot assign a value to %(lhs_name)s, type is %(lhs_type)s."
msgstr "%(lhs_type)s க்கு ஒரு மதிப்பை ஒதுக்க முடியாது, வகை %(lhs_name)s."

#: Mopy/bash/ScriptParser.py:1167
#, python-format
msgid "Cannot increment %(lhs_name)s, type is %(lhs_type)s."
msgstr "%(lhs_type)s அதிகரிக்க முடியாது, வகை %(lhs_name)s."

#: Mopy/bash/ScriptParser.py:1174
#, python-format
msgid "Cannot decrement %(lhs_name)s, type is %(lhs_type)s."
msgstr "%(lhs_name)s குறைக்க முடியாது, வகை %(lhs_type)s."

#: Mopy/bash/ScriptParser.py:1504
#, python-format
msgid "Cannot rename %(old_plugin_name)s to %(new_plugin_name)s: the extensions must match."
msgstr "%(old_plugin_name)s ஐ %(new_plugin_name)s மறுபெயரிட முடியாது: நீட்டிப்புகள் பொருந்த வேண்டும்."

#: Mopy/bash/ScriptParser.py:1538
msgid "Could not read the wizard file. Please ensure it is encoded in UTF-8 format."
msgstr "வழிகாட்டி கோப்பைப் படிக்க முடியவில்லை. இது யுடிஎஃப் -8 வடிவத்தில் குறியாக்கம் செய்யப்பட்டுள்ளதா என்பதை உறுதிப்படுத்தவும்."

#: Mopy/bash/ScriptParser.py:1541
msgid "Could not open the wizard file."
msgstr "வழிகாட்டி கோப்பைத் திறக்க முடியவில்லை."

#: Mopy/bash/archives.py:73 Mopy/bash/archives.py:86
